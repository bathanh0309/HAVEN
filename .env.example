# ================================
# HAVEN Environment Variables Template
# ================================
# 
# HƯỚNG DẪN SỬ DỤNG:
# 1. Copy file này thành .env:
#    copy .env.example .env
# 
# 2. Điền thông tin thật của camera vào file .env
# 
# 3. KHÔNG BAO GIỜ commit file .env lên Git!
#
# ================================

# ================================
# CAMERA NETWORK
# ================================
CAMERA_NAME=tapo_c210_example
CAMERA_IP=192.168.1.XXX          # ⚠️ Thay XXX bằng IP thật
CAMERA_PORT=554
CAMERA_USER=your_username         # ⚠️ Thay bằng username thật
CAMERA_PASSWORD=your_password     # ⚠️ Thay bằng password thật

# ================================
# RTSP STREAMS
# ================================
RTSP_STREAM_HD=stream1
RTSP_STREAM_SD=stream2
DEFAULT_STREAM=SD

# ================================
# AI MODEL
# ================================
AI_MODEL_PATH=models/yolo26n-pose.pt

# ================================
# PERFORMANCE TUNING
# ================================
FRAME_BUFFER_SIZE=1
RECONNECT_BACKOFF_SECONDS=5
MAX_RECONNECT_ATTEMPTS=10
STREAM_TIMEOUT_SECONDS=30
FPS_LIMIT=15
JPEG_QUALITY=80
RESIZE_WIDTH=0

# ================================
# ADVANCED OPTIONS
# ================================
DEBUG_MODE=False
WS_PING_INTERVAL=30
